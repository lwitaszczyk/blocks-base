{
    "name": "blocks/core",
    "type": "library",
    "description": "Blocks Core",
    "homepage": "http://vld.pl/",
    "license": "proprietary",
    "require": {
        "php": ">=5.5.0",
        "doctrine/orm": "2.5.*",
        "monolog/monolog": "*",
        "doctrine/migrations": "^1.0",
        "ocramius/proxy-manager": "1.0.*"
    },
    "require-dev": {
        "psr/log": "^1.0",
        "squizlabs/php_codesniffer": "2.*",
        "sebastian/phpcpd": "2.0.*",
        "sebastian/phpdcd": "1.0.*",
        "phpmd/phpmd": "2.2.*",
        "phploc/phploc": "2.1.*",
        "phpunit/phpunit": "4.6.*",
        "phpspec/phpspec": "2.2.*",
        "fabpot/php-cs-fixer": "^1.10"
    },
    "autoload": {
        "psr-0": {
            "" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Blocks\\": "tests/Tests/Blocks"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
